<h1>Listing proyectos</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Fproyecto</th>
    <th>Expediente</th>
    <th></th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @proyectos.each do |proyecto| %>
  <tr>
    <td><%= proyecto.nombre %></td>
    <td><%= proyecto.fproyecto %></td>
    <td><%= proyecto.expediente %></td>
    <td><%= link_to 'Selecciona', :controller=>'puntos', :action=>'index', :proyecto_id=>proyecto.id %></td>
    <td><%= link_to 'Show', proyecto %></td>
    <td><%= link_to 'Edit', edit_proyecto_path(proyecto) %></td>
    <td><%= link_to 'Destroy', proyecto, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Proyecto', new_proyecto_path %>
