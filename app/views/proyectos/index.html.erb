<h2>Listado de Proyectos</h2>

<div>
	<table>
	  <tr>
	    <th class="thdata">Nombre</th>
	    <th class="thdata">Expediente</th>
	    <th class="thdata">F Proyecto</th>
	    <th class="hdaction"></th>
	    <th class="hdaction"></th>
	    <th class="hdaction"></th>
	    <th class="hdaction"></th>
	  </tr>
	
	<% @proyectos.each do |proyecto| %>
	  <tr>
	    <td class="proyname"><%= proyecto.nombre %></td>
	    <td class="proyexp"><%= proyecto.expediente %></td>
	    <td><%= proyecto.fproyecto %></td>
	    <td class="tdaction"><%= link_to 'Selecciona', :controller=>'puntos', :action=>'index', :proyecto_id=>proyecto.id %></td>
	    <td class="tdaction"><%= link_to 'Show', proyecto %></td>
	    <td class="tdaction"><%= link_to 'Edit', edit_proyecto_path(proyecto) %></td>
	    <td class="tdaction"><%= link_to 'Destroy', proyecto, :confirm => 'Are you sure?', :method => :delete %></td>
	  </tr>
	<% end %>
	</table>
	
	<br />
	
	<%= link_to 'New Proyecto', new_proyecto_path %>
</div>