<h1>Listing ttramos</h1>

<table>
  <tr>
    <th>Tipo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ttramos.each do |ttramo| %>
  <tr>
    <td><%= ttramo.tipo %></td>
    <td><%= link_to 'Show', ttramo %></td>
    <td><%= link_to 'Edit', edit_ttramo_path(ttramo) %></td>
    <td><%= link_to 'Destroy', ttramo, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

